{
  "name": "movies-demo-backend",
  "version": "1.0.0",
  "description": "A web application for managing movies. Allows users to create, update, and display movie information.",
  "main": "src/index.js",
  "keywords": [
    "Movies"
  ],
  "author": "Ashish Manani",
  "license": "ISC",
  "scripts": {
    "start": "cross-env NODE_ENV=development node src/index.js",
    "dev": "cross-env NODE_ENV=development nodemon src/index.js",
    "prettier": "prettier --write '**/*.{js,md}'",
    "lint": "eslint **/*.js --fix"
  },
  "lint-staged": {
    "*.js": [
      "eslint --fix",
      "prettier --write"
    ],
    "*.md": [
      "prettier --write"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "dependencies": {
    "@apollo/server": "^4.11.0",
    "@aws-sdk/client-s3": "^3.633.0",
    "@aws-sdk/s3-request-presigner": "^3.633.0",
    "apollo-server-express": "^3.13.0",
    "bcrypt": "^5.1.1",
    "bcryptjs": "^2.4.3",
    "cross-env": "^7.0.3",
    "dotenv": "^16.4.5",
    "express": "^4.19.2",
    "graphql": "^16.9.0",
    "graphql-middleware": "^6.1.35",
    "graphql-shield": "^7.6.5",
    "joi": "^17.13.3",
    "jsonwebtoken": "^9.0.2",
    "migrate-mongo": "^11.0.0",
    "moment": "^2.30.1",
    "mongodb": "^6.8.0",
    "mongoose": "^8.5.3",
    "validator": "^13.12.0",
    "winston": "^3.14.2"
  },
  "devDependencies": {
    "@eslint/js": "^9.9.0",
    "eslint": "^9.9.0",
    "globals": "^15.9.0",
    "husky": "^9.1.5",
    "lint-staged": "^15.2.9",
    "nodemon": "^3.1.4",
    "prettier": "^3.3.3"
  }
}
